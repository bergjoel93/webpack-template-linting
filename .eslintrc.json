{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn"
  },

  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error"
  }
}
